📌 ##输入10个整数，从小到大排序
#include<stdio.h>
int main()
{
	int a[10],i,j,t;                          /*输入n个数，a[0]到a[n-1]*/
	int n=10;
	for(i=0;i<n;i++)                          /*0~n-1，n个数*/
		scanf("%d ",&a[i]);
	for(j=0;j<(n-1);j++)                      /*排序n-1次*/
		for(i=0;i<(n-1-j);i++)
			if (a[i]>a[i+1])	                    /*如果前面的数大*/
			{t=a[i];a[i]=a[i+1];a[i+1]=t;         /*📌交换值：令t=前一个大数，把后面的小数值赋给前面，再把大数值t赋给后面*/
			}                                     /*无else，前面数值小则不变*/
		for(i=0;i<10;i++)                       /*输出10个数*/
			printf("%d ",a[i]);
	return 0;
}
